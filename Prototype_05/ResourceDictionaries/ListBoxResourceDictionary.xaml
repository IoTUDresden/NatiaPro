    																																</BeginStoryboard>
    																															</Trigger.EnterActions>
    																															<Trigger.ExitActions>
    																																<BeginStoryboard>
    																																	<Storyboard>
    																																		<DoubleAnimation Duration="0:0:0.1" To="14" Storyboard.TargetProperty="Height" Storyboard.TargetName="Thumb"/>
    																																		<ThicknessAnimation Duration="0:0:0.1" To="0" Storyboard.TargetProperty="Margin" Storyboard.TargetName="Thumb"/>
    																																	</Storyboard>
    																																</BeginStoryboard>
    																															</Trigger.ExitActions>
    																														</Trigger>
    																														<Trigger Property="IsEnabled" Value="False">
    																															<Setter Property="Fill" TargetName="Thumb" Value="{DynamicResource {ComponentResourceKey TypeInTargetAssembly={x:Type Custom:SurfaceColors}}}"/>
    																														</Trigger>
    																													</ControlTemplate.Triggers>
    																												</ControlTemplate>
    																											</Setter.Value>
    																										</Setter>
    																										<Style.BasedOn>
    																											<Style TargetType="{x:Type Control}">
