        <ControlTemplate x:Key="TextBoxTemplateDisable" TargetType="TextBox">
            <Border Name="Bd" BorderThickness="{TemplateBinding BorderThickness}" 
                                             BorderBrush="{TemplateBinding BorderBrush}" 
                                             Background="{TemplateBinding Background}" 
                                             SnapsToDevicePixels="true">
                <ScrollViewer Name="PART_ContentHost" Background="{TemplateBinding Background}" 
                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter Value="{x:Null}" Property="Background" />
                    <Setter Value="Gray" Property="Foreground" />
                    <Setter TargetName="PART_ContentHost" Property="Background" Value="{x:Null}"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
		
		
        <ControlTemplate x:Key="ToggleTextBoxTemplateDisable" TargetType="TextBox">
            <Border Name="Bd" BorderThickness="{TemplateBinding BorderThickness}" 
                                             BorderBrush="{TemplateBinding BorderBrush}" 
                                             Background="{TemplateBinding Background}" 
                                             SnapsToDevicePixels="true">
                <ScrollViewer Name="PART_ContentHost" Background="{TemplateBinding Background}" 
                                          SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" />
            </Border>
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled" Value="False">
                    <Setter Value="{x:Null}" Property="Background" />
                    <Setter Value="#FF9D9D9D" Property="Foreground" />
                    <Setter Value="Normal" Property="FontWeight" />
                    <Setter TargetName="PART_ContentHost" Property="Background" Value="{x:Null}"/>
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
